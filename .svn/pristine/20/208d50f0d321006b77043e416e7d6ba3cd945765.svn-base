@CHARSET "UTF-8";
html{
scrollbar-face-color: white;
scrollbar-highlight-color: #D6D6D6;
scrollbar-shadow-color: #D6D6D6;
scrollbar-3dlight-color: white;
scrollbar-arrow-color: #D6D6D6;
scrollbar-track-color: white;
scrollbar-darkshadow-color: white; 
SCROLLBAR-BASE-COLOR:#F1F1F1;
}
*{font-size:14px;border:0;margin:0;padding:0;}
body{font-family:'Microsoft YaHei'; margin:0 auto;min-width:980px;}
ul{display:block;margin:0;padding:0;list-style:none;}
li{display:block;margin:0;padding:0;list-style: none;}
img{border:0;}
a,a:focus{text-decoration:none;color:#000;outline:none;blr:expression(this.onFocus=this.blur());}
a:hover{color:#00a4ac;text-decoration:none;}
cite{font-style:normal;}
h2{font-weight:normal;}
input::-ms-clear {display:none;}
#mainBody {width:100%;height:100%;position:absolute;z-index:-1;}
.cloud {position:absolute;top:0px;left:0px;width:100%;height:100%;background:url(../images/login/cloud.png) no-repeat;z-index:1;opacity:0.5;}
#cloud2 {z-index:2;}

/*登陆窗口*/
.logintop{height:47px; position:absolute; top:0; background:url(../images/login/loginbg1.png) repeat-x;z-index:100; width:100%;}
.logintop span{margin:0;padding:0;color:#fff; line-height:47px; background:url(../images/login/loginsj.png) no-repeat 21px 18px; text-indent:44px; color:#FFF; float:left;font-size:14px;}
.logintop ul{float:right; padding-right:10px;}
.logintop ul li{float:left; margin-left:10px; line-height:47px;}
.logintop ul li a{color:#afc5d2;}
.logintop ul li a:hover{color:#fff;}
.loginbody{background:url(../images/login/loginbg3.png) no-repeat center center; width:100%; height:585px; overflow:hidden; position:absolute; top:47px;}
/* .systemlogo{background:url(../images/login/loginlogo.png) no-repeat center;text-align:center;font-size:38pt;font-family:'Microsoft YaHei';color:#cbe7f2;width:100%; height:71px; margin-top:75px;} */
.systemlogo{text-align:center;font-size:38pt;font-family:'Microsoft YaHei';color:#cbe7f2;width:100%; height:71px; margin:75px auto 0px auto;}
.loginbox{width:692px; height:336px; background:url(../images/login/logininfo.png) no-repeat; margin-top:30px;}
.loginbox ul{margin-top:88px; margin-left:285px;list-style:none;}
.loginbox ul li{margin-bottom:15px;list-style: none;}
.loginbox ul li img{vertical-align:middle;}
.loginbox ul li label{color:#687f92; padding-left:25px;}
.loginbox ul li label a{color:#687f92;}
.loginbox ul li label a:hover{color:#3d96c9;}
.loginbox ul li label input{margin-right:5px;}
.loginuser{width:299px; height:35px; background:url(../images/login/loginuser.png) no-repeat; border:none;outline:none; line-height:35px; padding-left:44px; font-size:14px; color:#90a2bc;font-weight:bold;}
.loginpwd{width:299px; height:35px; background:url(../images/login/loginpassword.png) no-repeat; border:none;outline:none;line-height:35px; padding-left:44px; font-size:14px; color:#90a2bc;font-weight:bold;}
.logincaptcha{width:180px; height:35px; background:url(../images/login/logincaptcha.png) no-repeat; border:none;outline:none;line-height:35px; padding-left:44px; font-size:14px; color:#90a2bc;font-weight:bold;}

.loginbtn{width:111px;height:35px; background:url(../images/login/buttonbg.png) repeat-x; font-size:14px;font-family:'Microsoft YaHei'; font-weight:bold; color:#fff;cursor:pointer; line-height:35px;}
.loginbm{height:50px; line-height:50px; text-align:center; background:url(../images/login/loginbg2.png) repeat-x;position:absolute; bottom:0; width:100%; color:#0b3a58;}
.loginbm a{font-weight:bold;color:#0b3a58;}
.loginbm a:hover{color:#fff;}
.lostpwd {font-size:14px;font-family:'Microsoft YaHei';}
/*头部*/
#mainheader{height:55px;background:url(../images/top/topbg.gif) repeat-x;width:100%}
.topleft{height:55px;background:url(../images/top/topleft.jpg) no-repeat;float:left; width:350px;color:#fbfbfd;height:55px;line-height:55px;font-size:24pt;font-weight:bold;font-family:'黑体'}
.topleft img{margin-top:12px;margin-left:10px;}
.topright{height:55px;background:url(../images/top/topright.jpg) no-repeat right;float:right;width:600px;height:55px;line-height:55px;}
.topright ul{padding-top:0px; float:right; padding-right:20px;height:55px;line-height:55px}
.topright ul li{float:left; padding-left:9px; padding-right:9px; background:url(../images/top/line.gif) no-repeat right;}
.topright ul li:last-child{background:none;}
.topright ul li a{font-size:13px; color:#e9f2f7;}
.topright ul li a:hover{color:#fff;}
.topright ul li span{margin-top:2px;float:left;padding-right:3px;}
.user{height:30px;line-height:30px;background:url(../images/top/ub1.png) repeat-x;clear:both;margin-top:10px;float:left; margin-right:12px;border-radius:30px; behavior:url(js/pie.htc); white-space:nowrap;position:relative;}
.user #username{display:inline-block;padding-right:10px;cursor:hand;cursor:pointer; background:url(../images/top/user.png) no-repeat 15px 10px; line-height:30px; font-size:14px;color:#b8ceda; padding-left:20px; padding-left:35px;}
.user #untiname{display:inline-block;padding-right:10px;cursor:hand;cursor:pointer; background:url(../images/top/ent.png) no-repeat 15px 10px; line-height:30px; font-size:14px;color:#b8ceda; padding-left:20px; padding-left:35px;}

#menufirst{
	
	width:100%;
	height:31px;
	margin:0px;
