/*编辑框*/
.input
{
	font-family: Arial, Sans-Serif;
	font-size: 14px;
	border:1px solid #d9d9d9;
	height:25px;	
	width: 252px;
	background-color:#FFFFFF;
	border-color:#b1b3b2;
}
.input:hover{
	border-color:#fcef9a;
	background-color:#f8fafc;
}
/*多行编辑框*/
.textarea{		
	overflow:auto;
	width:252px;
	height:80px;
	color:#000000;
	border-color:#d9d9d9;
	border-style:solid;
	border-width:1px;
	font-size:14px;
}
.textarea:hover{border-color:#fcef9a;
background-color:#f8fafc;}
/*日期编辑框*/
.date{	
	background-image:url(../images/form/datePicker.gif)!important;
	background-repeat:no-repeat!important;
	background-position:100% 50%!important;
	height:25px;
	line-height:25px;
	width:252px;
	border-style:solid;
	border-width:1px;
	font-size:14px;
	color:#000000;
	border-color:#d9d9d9;
}
.date:hover{border-color:#fcef9a;
background-color:#f8fafc;}
.inputselect{	
	background-image:url(../images/icons/search.gif)!important;
	background-repeat:no-repeat!important;
	background-position:100% 50%!important;
	height:25px;
	line-height:25px;
	width:252px;
	border-style:solid;
	border-width:1px;
	font-size:14px;
	color:#000000;
	border-color:#d9d9d9;
}
.inputselect:hover{border-color:#fcef9a;
background-color:#f8fafc;}
/*日期时间编辑框*/
.datetime{	
	background-image:url(../images/form/icon_clock_1.gif)!important;
	background-repeat:no-repeat!important;
	background-position:100% 50%!important;
	height:25px;
	line-height:25px;
	width:250px;
	border-style:solid;
	border-width:1px;
	font-size:14px;
	color:#000000;
	border-color:#d9d9d9;
}
.datetime:hover{border-color:#fcef9a;
background-color:#f8fafc;
}
/*水印*/
.watermark{color:#999!important;}
.edittable{
	BACKGROUND-COLOR: #FFFFFF;
	margin: 0px;	
	line-height: 30px;
	font-size:14px;
	font-family:'宋体';
	border-collapse:collapse;
	border-spacing:0;
	width:100%;
	table-layout:fixed;
}

.righttr{
	border: 1px dashed #dcf8a8;
	BACKGROUND-COLOR: #fbfbfb;	
    text-align: right;
	padding-right:2px;
	font-size:16px;
	font-family:'微软雅黑';
	width:130px;
	border:0px;
}

.required{	
	background-color:#FFFFFF;
	background-image:url(../images/form/required2.gif);
	background-repeat:no-repeat;
	background-position:100% 50%;
	height:25px;
	line-height:25px;
	width:250px;
	border-color:#d9d9d9;
	border-style:solid;
	border-width:1px;
	font-size:14px;
	color:#000000;
}
.required:hover{border-color:#fcef9a;
background-color:#f8fafc;
}
/*光标*/
.hand{cursor:pointer;cursor:hand;}
a:link  {
	FONT-FAMILY:Arial, Sans-Serif,'宋体';
    font-size: 12px;
    color: #000000;
    text-decoration: none;
}

a:visited {
	color: #000000;
	text-decoration: none;
}

a:hover {
	color: #000000;
	text-decoration: none;
}

a:active {
	color: #000000;
	text-decoration: none;
}

.edittable .left{
	BACKGROUND-COLOR: #FFFFFF;
	border: 1px dashed #dcf8a8;
    text-align: left;
	padding-left:2px;
	white-space:nowrap;
}
.edittable .right{
	border: 1px dashed #dcf8a8;
	BACKGROUND-COLOR: #fbfbfb;	
    text-align: right;
	padding-right:2px;
	font-size:14px;
	font-family:'微软雅黑';
	width:90px;
}
.edittable .center{
	border: 1px dashed #ffffff;
	BACKGROUND-COLOR: #B8D3F1;	
    text-align: center;
}
.edittable .buttonrow{
	margin-bottom: 1px;
	margin-bottom: 2px;
    height: 30px;
}

