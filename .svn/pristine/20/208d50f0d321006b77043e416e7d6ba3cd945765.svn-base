/**
*Panel 面板样式
*/
.panel {display:block; background-position:0 100%;}
.panel .panelHeader { 
	background:url(../images/layout/panel-left.gif) no-repeat;
	display:block; 
	height:38px; 
	padding-left:5px; 
		
}
.panel .panelHeaderContent { 
	display:block; 
	background:url(../images/layout/panel-right.gif) repeat-x;
	height:38px; 
	padding-right:5px; 
	position:relative;	
}
.panel .panelHeaderContent h1 {/*面板标题的样式*/ 
	display:block; 
	overflow:hidden; 
	height:38px; 
	padding:0 5px;
	line-height:36px; 
	background:url(../images/layout/panel-center.gif);
	background-repeat:repeat-x;
	font-size: 14px;
	font-family:'微软雅黑';
	color:#777eac;	
}
.panel .panelContent {/*内容样式*/
	display:block; 
	overflow:auto; 
	padding:1px 1px 1px 5px; 
	background:#ffffff;
	border-color:#E7EAEE;
	border-style:solid; 
	border-width:0px 1px 1px 1px;
}
.panel .collapsable, .panel .expandable { 
	display:block; 
	overflow:hidden; 
	width:20px; 
	height:21px; 
	text-indent:-1000px; 
	position:absolute; 
	top:8px; 
	right:6px;
	cursor:hand;
	cursor:pointer;
	background:url(../images/layout/panel_icon.gif) no-repeat;
}
.panel .collapsable:hover { cursor:hand;background-position:0 -50px;}
.panel .expandable { cursor:hand;background-position:0 -100px;}
.panel .expandable:hover { cursor:hand;background-position:0 -150px;}
