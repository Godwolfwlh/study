	cursor:pointer;
}
/*企业和供应商菜单*/

#menufirst .selected{
	
	width:100px;
	height:31px;
	line-height:30px;
	text-align:center;
	color:#FFFFFF;
	font-size:14px;
	font-family:"微软雅黑";
	cursor:hand;
	cursor:pointer;
	float:left;
	background-image:url(../images/right/selected.gif);
    background-repeat:repeat-x;  
	font-weight:bold;
}
