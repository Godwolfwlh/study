/* -----------------------
*   下拉框样式 *
* ----------------------- */
.selectbox-wapper{
	white-space: nowrap;
	overflow:hidden;
	font-size:14px;
	font-family:'宋体';	
}
.selectbox
{
    position:relative;
    border:1px solid #d9d9d9; 
    height:25px; 
    line-height:25px;
    width:250px; 
    font-size:14px;
	font-family:'宋体';	
    background:white;
}
.selectbox-field
{
    position:absolute; top:1px; left:1px;
    width:240px; 
    font-size:14px;
	font-family:'宋体';	
	height:24px; 
    line-height:24px;
    border:0; margin:0; outline:none;  
}
.selectbox-trigger,.selectbox-trigger-hover,.selectbox-trigger-pressed,.selectbox-trigger-disabled
{
    cursor:pointer;
    position:absolute;
    top:1px;right:1px;
    width:16px;height:21px;
    overflow:hidden;
    
}
.selectbox-trigger
{
    border:1px solid #d9d9d9; 
    background:#DBEBFF url('../images/combotree/bg-trigger.gif') repeat-x left;
    
}
.selectbox-trigger-disabled
{
    border:1px solid #dadada; 
    background:#DBEBFF url('../images/combotree/bg-trigger.gif') repeat-x left;
}
.selectbox-trigger-hover
{
    border:1px solid #EABE4E; 
    background:#fedc75 url('../images/combotree/bg-trigger-over.gif') repeat-x left;
}
.selectbox-trigger-pressed
{
    border:1px solid #EABE4E; 
    background:#fedc75 url('../images/combotree/bg-trigger-pressed.gif') repeat-x left;

}
.selectbox-icon
{	width:100%;
	height:100%;
    background:url('../images/combotree/icon-down.gif') no-repeat 50% 50%;
}
.selectbox-list
{
	border-top:1px solid #d9d9d9;	
    display:none;
    position:absolute; 
	padding:0px; 
    font-size:14px; 
	line-height:25px;	
	z-index:1004;
}
.selecttree-list
{
    display:none;
    position:absolute; 
	padding:0px; 
    z-index:1004; font-size:14px; 
	line-height:25px;	
	background-color:white;
	text-align: left; 
	vertical-align: top; 
	
}
.selectitems {
	list-style-type:none;
	margin:0;
	padding:0;
	margin-bottom:8px;
	background-color:white;
	text-align: left; 
	vertical-align: top; 
	cursor: default; 
	border-bottom:1px solid #d9d9d9; 
	border-left:1px solid #d9d9d9; 
	border-right:1px solid #d9d9d9;
	border-top:0px solid #d9d9d9;
	overflow-x: hidden; 
	overflow-y: auto; width: 180px; height: 200px;
	
}
.selectitems li{
	padding-bottom:0px;
}
.selectitems li a{
	height:25px;
	line-height:25px;
	background-color:white;
	color:#000000;
	display:block;
	text-decoration:none;	
	width:100%;
	font-size:14px;
	vertical-align: center; 
}
.combo-selected{
	height:25px;
	line-height:25px;
	font-size:14px;
	background-color:#ffebbf;	
}
.combo-selected span{	
	background-color:#fcef9a;
	line-height:25px;
	height:25px;
}
.selectitems li a:hover{
	background:#f9f9f9;
	color:black;
	text-decoration:none;
}
.selectitems li span{	
	display:block;
	padding-top:0px;
	padding-bottom:3px;
	padding-left:3px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	font-size:14px;
	color:#000000;
	line-height:25px;
	height:25px;
}

